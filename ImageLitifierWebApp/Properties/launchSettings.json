{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:41039",
      "sslPort": 44302
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5156",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BLOB_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true",
        "BLOB_STORAGE_REQUESTS_CONTAINER_NAME": "image-requests",
        "BLOB_STORAGE_RESPONSES_CONTAINER_NAME": "image-responses",
        "SERVICE_BUS_CONNECTION_STRING": "Endpoint=sb://localhost:9350/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your_shared_access_key",
        "SERVICE_BUS_QUEUE_NAME": "image-requests"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:7252;http://localhost:5156",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BLOB_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true",
        "BLOB_STORAGE_REQUESTS_CONTAINER_NAME": "image-requests",
        "BLOB_STORAGE_RESPONSES_CONTAINER_NAME": "image-responses",
        "SERVICE_BUS_CONNECTION_STRING": "Endpoint=sb://localhost:9350/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your_shared_access_key",
        "SERVICE_BUS_QUEUE_NAME": "image-requests"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "BLOB_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true",
        "BLOB_STORAGE_REQUESTS_CONTAINER_NAME": "image-requests",
        "BLOB_STORAGE_RESPONSES_CONTAINER_NAME": "image-responses",
        "SERVICE_BUS_CONNECTION_STRING": "Endpoint=sb://localhost:9350/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your_shared_access_key",
        "SERVICE_BUS_QUEUE_NAME": "image-requests"
      }
    }
  }
}
